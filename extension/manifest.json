{
  "manifest_version": 3,
  "name": "listening to",
  "version": "0.1",
  "description": "youtube music discord rich presence bridge",
  "background": {
    "service_worker": "background.js",
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://music.youtube.com/*"],
      "js": ["watcher.js"]
    }
  ]
}
